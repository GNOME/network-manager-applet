foreach icon: icons
  icon_dir = join_paths(nma_datadir, 'icons', 'hicolor', icon[1], 'apps')
  foreach name: icon[2]
    install_data(
      join_paths(icon[0], name),
      install_dir: icon_dir
    )
  endforeach
endforeach
